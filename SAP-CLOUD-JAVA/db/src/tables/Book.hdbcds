namespace javaSAM;

entity Book {
    @Comment: 'Id of Book'
    key id : Integer64 generated always as identity ( start with 1 increment by 1 );
    @Comment: 'Caption'
    caption : String(20);
    @Comment: 'Description'
    description : String(100);
    @Comment: 'Number of pages'
    pages : Integer;
};
